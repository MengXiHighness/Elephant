<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ig="http://schemas.infragistics.com/xaml"
                    >
    <Style x:Key="headIconLeftStyle" TargetType="{x:Type Grid}"> 
        <Setter Property="Background"  >
            <Setter.Value>
                <ImageBrush  Stretch="Fill" ImageSource="/DS.AFP.WPF.App;component/Resources/Themes/Grey/Images/headIconLeft.png" ></ImageBrush>
            </Setter.Value>
        </Setter>
    </Style>
